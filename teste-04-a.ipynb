   "source": [
    "import numpy as np\n",
    "\n",
    "def f(t):\n",
    "  return np.e**(-t**2)\n",
    "\n",
    "h = 1\n",
    "X = np.arange(0, 10, h)\n",
    "\n",
    "simpson = np.sum(h/6 * (f(X) + 4 * f(X + h/2) + f(X + h)))\n",
    "\n",
    "integral = 2/np.sqrt(np.pi) * simpson\n",
    "\n",
    "print (integral)\n",
    "\n"
   ]
  }
